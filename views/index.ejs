<html>
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="styles.css">
    <title><%= title %></title>
  </head>
  <body>
    <h1><%=title%></h1>
    <% messages.forEach((message) => { %>
    <h2><%=message.user%></h2>
    <h3><%=message.added%></h3>
    <p><%=message.text%></p>
    <%}) %>
  </body>
</html>
